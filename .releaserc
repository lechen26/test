{
  "plugins": [ 
    ["@semantic-release/commit-analyzer", {   
      "releaseRules": [
        {"merge":"Merge","release":"minor"}
      ],
       "parserOpts": {
         "mergePattern": "/[mM]erge (.*) #(\\d+) from (.*)$/",
          "mergeCorrespondence": ["plain", "id", "source"]
        }
      }],
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm"
  ]
}
